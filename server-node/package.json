{
  "name": "api-tablu-tasks",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "tsx watch --include ./.env src/index.ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:ts": "tsc --noEmit -p ./tsconfig.json",
    "lint": "npm run lint:fix && npm run lint:ts",
    "migrate:dev": "docker exec -it api-tablu-tasks npx prisma migrate dev --schema=./prisma/schema.prisma",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "8",
    "eslint-config-prettier": "^10.1.2",
    "prisma": "^6.6.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "tsx": "^4.19.3",
    "zod": "^3.24.3"
  }
}
